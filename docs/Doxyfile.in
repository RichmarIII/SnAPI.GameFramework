PROJECT_NAME           = "SnAPI.GameFramework"
PROJECT_BRIEF          = "Data-driven gameplay framework with reflection, serialization, replication, and asset integration"
OUTPUT_DIRECTORY       = "@SNAPI_GF_DOXYGEN_OUTPUT@"
HTML_OUTPUT            = html
INPUT                  = @SNAPI_GF_DOXYGEN_INPUT@
FILE_PATTERNS          = *.h *.hpp *.cpp *.md
RECURSIVE              = YES
STRIP_FROM_PATH        = "@CMAKE_SOURCE_DIR@"
STRIP_FROM_INC_PATH    = "@CMAKE_SOURCE_DIR@"
FULL_SIDEBAR           = NO
GENERATE_TREEVIEW      = NO
TOC_INCLUDE_HEADINGS   = 3
HTML_DYNAMIC_SECTIONS  = NO
HTML_COLORSTYLE        = LIGHT
MARKDOWN_SUPPORT       = YES
AUTOLINK_SUPPORT       = YES
BUILTIN_STL_SUPPORT    = YES
JAVADOC_AUTOBRIEF      = YES
MULTILINE_CPP_IS_BRIEF = YES
EXTRACT_ALL            = YES
EXTRACT_PRIVATE        = YES
EXTRACT_STATIC         = YES
EXTRACT_LOCAL_CLASSES  = YES
EXTRACT_ANON_NSPACES   = YES
SOURCE_BROWSER         = YES
REFERENCED_BY_RELATION = YES
REFERENCES_RELATION    = YES
REFERENCES_LINK_SOURCE = YES
WARN_IF_UNDOCUMENTED   = YES
WARN_IF_DOC_ERROR      = YES
WARN_NO_PARAMDOC       = YES
GENERATE_HTML          = NO
GENERATE_XML           = YES
XML_OUTPUT             = xml
GENERATE_LATEX         = NO
GENERATE_MAN           = NO
GENERATE_RTF           = NO
SEARCHENGINE           = NO
